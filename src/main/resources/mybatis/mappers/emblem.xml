<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="emblem">
	
	<select id="selectUidEmblem" parameterType="string" resultType="emblemVO">
		SELECT *
		  FROM emblem_achive
		 WHERE uid=#{uid}
	</select>
	
</mapper>